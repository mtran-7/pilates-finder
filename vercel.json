{
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/states", "destination": "/states.html" },
    { "source": "/:state", "destination": "/cities.html" },
    { "source": "/:state/:city", "destination": "/city.html" },
    { "source": "/about-pilates-finder", "destination": "/about.html" },
    { "source": "/contact-us", "destination": "/contact.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/([^/]+)(?:/([^/]+))?(?:/([^/]+))?",
      "dest": "/$1",
      "continue": true,
      "check": true
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
