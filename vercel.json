{
  "rewrites": [
    { "source": "/states", "destination": "/states.html" },
    { "source": "/contact-us", "destination": "/contact.html" },
    { "source": "/about-pilates-finder", "destination": "/about.html" },
    { "source": "/:state([a-z0-9-]+)", "destination": "/cities.html" },
    { "source": "/:state([a-z0-9-]+)/:city([a-z0-9-]+)", "destination": "/city.html" },
    { "source": "/(.*)", "status": 404, "destination": "/404.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}