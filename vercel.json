{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/states",
      "dest": "/states.html"
    },
    {
      "src": "/[state]",
      "dest": "/[state]/cities.html"
    },
    {
      "src": "/[state]/[city]",
      "dest": "/city.html"
    },
    {
      "src": "/[state]/[city]/[studio]",
      "dest": "/studio.html"
    },
    {
      "src": "/about-pilates-finder",
      "dest": "/about.html"
    },
    {
      "src": "/contact-us",
      "dest": "/contact.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}